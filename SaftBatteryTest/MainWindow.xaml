<Window x:Class="SaftBatteryTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SaftBatteryTest"
        xmlns:ctr="clr-namespace:SaftBatteryTest.View.Controls"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <ctr:Channel Grid.Row="0" MyText="{Binding Body.Title}"></ctr:Channel>
        <Button Grid.Row="1" Command="{Binding TestCommand}"></Button>
    </Grid>
</Window>
